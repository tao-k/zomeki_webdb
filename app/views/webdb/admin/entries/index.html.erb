<table class="index">
  <tr>
    <th style="text-align: center; width: 60px;">詳細</th>
    <th><%= Webdb::Item.human_attribute_name :title %></th>
    <th style="text-align: center; width: 150px;"><%= Webdb::Entry.human_attribute_name :created_at %></th>
  </tr>
  <%- @items.each do |item| -%>
  <tr class="<%= cycle '', 'cycle' %>">
    <td style="text-align: center;"><%= link_to :show, webdb_db_entry_path(content: @content, db_id: @db.id, id: item.id) %></td>
    <td style="text-align: left;"><%= item.title %></td>
    <td style="text-align: center;"><%=l item.created_at %></td>
  </tr>
  <%- end -%>
</table>

<%= paginate @items -%>
