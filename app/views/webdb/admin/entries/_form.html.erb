  <table class="show">
    <tr>
      <th><%= f.label :editor_id %></th>
      <td><%#= select_tag 'item[editor_id]',
                         options_from_collection_for_select(Login::Content::User.where(site_id: Core.site.id), :id, :name, @item.member_content_id), include_blank: true %></td>
     </tr>
   </table>
<% end %>
