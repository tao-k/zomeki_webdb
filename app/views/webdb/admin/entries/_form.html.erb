  <tr>
    <th><%= f.label :title %> <span class="note">※</span></th>
    <td><%= f.text_field :title, :class => 'title' %></td>
  </tr>
</table>

<%= render 'webdb/admin/shared/form', db: @item.db, item_values: @item.item_values if @item.db %>

